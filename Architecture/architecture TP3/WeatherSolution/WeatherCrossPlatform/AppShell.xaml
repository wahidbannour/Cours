<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="WeatherCrossPlatform.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:WeatherCrossPlatform"
    xmlns:view="clr-namespace:WeatherCrossPlatform.View"
    Shell.FlyoutBehavior="Disabled"
    Title="WeatherCrossPlatform">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
    Title="Weather"
    ContentTemplate="{DataTemplate view:WeahterPage}"
    Route="WeatherPage" />

</Shell>
